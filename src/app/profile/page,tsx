"use client";

import { useEffect } from "react";
import { useRouter } from "next/navigation";

// Dummy "session"
const user = {
  name: "Jane Admin",
  role: "admin", // change to "user" to test
};

export default function ProfileRedirectPage() {
  const router = useRouter();

  useEffect(() => {
    if (user.role === "admin") {
      router.push("/admin");
    } else {
      router.push("/user");
    }
  }, [router]);

  
}
